<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.xyb.androidystspractice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".lauchMode.SingleTaskDemoActivity"
            android:launchMode="singleInstance">
        </activity>
        <activity android:name=".IntentFilterDemo.DesActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:mimeType="image/jpeg"
                    android:scheme="http"></data>
            </intent-filter>
        </activity>

        <activity android:name=".IntentFilterDemo.DesActivity2">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>

                <data
                    android:scheme="content"></data>
            </intent-filter>
        </activity>

        <activity android:name=".IntentFilterDemo.DesActivity3">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:mimeType="text/plain"></data>
            </intent-filter>
        </activity>
    </application>

</manifest>